{
    "name": "arukompas/better-log-viewer",
    "description": "Fast and easy-to-use log viewer for your Laravel application",
    "keywords": [
        "arukompas",
        "laravel",
        "better-log-viewer"
    ],
    "homepage": "https://github.com/arukompas/better-log-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "Arunas Skirius",
            "email": "arukomp@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "itsgoingd/clockwork": "^5.1",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Arukompas\\BetterLogViewer\\": "src",
            "Arukompas\\BetterLogViewer\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arukompas\\BetterLogViewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arukompas\\BetterLogViewer\\BetterLogViewerServiceProvider"
            ],
            "aliases": {
                "BetterLogViewer": "Arukompas\\BetterLogViewer\\Facades\\BetterLogViewer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
